<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!--服务监听端口-->
    <add key="Game.Port" value="9001" />
    <!--Code：游戏代码；ServerId：游戏分区代码-->
    <add key="Product.Code" value="1"/>
    <add key="Product.ServerId" value="1"/>
    <!--Python脚本配置
      Python_IsDebug：是否开启调试功能
      PythonRootPath：脚本路径，相对于程序运行目录
    -->
    <add key="Python_IsDebug" value="true"/>
    <add key="PythonRootPath" value="..\..\PyScript"/>
  </appSettings>

  <connectionStrings>
    <add name="GameData" providerName="" connectionString="Data Source=.;Initial Catalog=GameData;Integrated Security=True;"/>
  </connectionStrings>


  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
